{
  "name": "ss-ele",
  "version": "0.0.1",
  "private": false,
  "packageManager": "pnpm@7.1.9",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -C play dev",
    "gen:version": "tsx scripts/gen-version.ts",
    "update:version": "tsx scripts/update-version.ts",
    "clean": "pnpm run clean:lib && pnpm run -r --parallel clean",
    "clean:lib": "rimraf lib",
    "build": "pnpm run -C internal/build start",
    "build:theme": "pnpm run -C packages/theme-chalk build",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub && concurrently \"pnpm gen:version\"",
    "clear1": "rimraf 'packages/*/{lib,node_modules}' && rimraf 'internal/*/{lib,node_modules}'",
    "clear2": "rimraf 'play/node_modules' && rimraf node_modules && rimraf lib",
    "clear": "npm run clear1 && npm run clear2",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "@element-plus/icons-vue": "^2.0.5",
    "@floating-ui/dom": "^0.5.2",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@ss-ele/components": "workspace:*",
    "@ss-ele/constants": "workspace:*",
    "@ss-ele/directives": "workspace:*",
    "@ss-ele/hooks": "workspace:*",
    "@ss-ele/locale": "workspace:*",
    "@ss-ele/theme-chalk": "workspace:*",
    "@ss-ele/tokens": "workspace:*",
    "@ss-ele/utils": "workspace:*",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@vue/shared": "^3.2.37",
    "@vueuse/core": "^8.6.0",
    "async-validator": "^4.1.1",
    "dayjs": "^1.11.3",
    "escape-html": "^1.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "memoize-one": "^6.0.0",
    "normalize-wheel-es": "^1.1.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.1",
    "@esbuild-kit/cjs-loader": "^2.0.1",
    "@pnpm/find-workspace-packages": "^4.0.10",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.0.1",
    "@ss-ele/build": "workspace:^0.0.1",
    "@ss-ele/build-utils": "workspace:^0.0.1",
    "@ss-ele/eslint-config": "workspace:*",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jsdom": "^16.2.14",
    "@types/node": "*",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/ui": "0.12.6",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.11.3",
    "chalk": "^5.0.1",
    "commitizen": "^4.2.4",
    "concurrently": "^7.2.1",
    "consola": "^2.15.3",
    "csstype": "^2.6.20",
    "cz-git": "^1.3.4",
    "eslint": "^8.17.0",
    "eslint-define-config": "^1.5.1",
    "expect-type": "^0.13.0",
    "fast-glob": "^3.2.11",
    "jsdom": "16.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.52.2",
    "ts-morph": "^14.0.0",
    "tsx": "^3.4.2",
    "type-fest": "^2.13.0",
    "typescript": "^4.7.3",
    "unplugin-vue-define-options": "^0.6.1",
    "vitest": "0.12.6",
    "vue": "^3.2.37"
  },
  "engines": {
    "node": ">= 16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
